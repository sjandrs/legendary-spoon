{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:codemod": "node scripts/lint/run-codemods.js",
    "lint:codemod:dry": "node scripts/lint/run-codemods.js --dry-run",
    "lint:codemod:tests": "node scripts/lint/run-codemods.js --tests-only",
    "lint:codemod:tests:dry": "node scripts/lint/run-codemods.js --tests-only --dry-run",
    "lint:audit:hooks": "node scripts/lint/audit-hooks.js",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:component": "cypress run --component",
    "test:a11y": "cypress run --spec 'cypress/e2e/**/*a11y*'",
    "test:performance": "lighthouse-ci autorun",
    "test:cross-browser": "npm run test:chrome && npm run test:firefox && npm run test:edge",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test:safari": "playwright test --project=webkit",
    "test:fast": "jest --ci --coverage --watchAll=false --maxWorkers=75% --bail",
    "test:parallel": "jest --ci --coverage --watchAll=false --maxWorkers=100%",
    "test:smoke": "jest --testPathPattern='smoke' --ci --bail",
    "i18n:extract": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:sync": "i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:check": "echo 'Checking translation keys...' && i18next-scanner --config i18next-scanner.config.cjs",
    "i18n:validate": "node scripts/validate-translations.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@headlessui/react": "^2.2.9",
    "@hello-pangea/dnd": "^18.0.1",
    "@jest/globals": "^30.2.0",
    "@react-spring/web": "^10.0.3",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "html2canvas": "^1.4.1",
    "i18next": "^24.2.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "jspdf": "^3.0.3",
    "leaflet": "^1.9.4",
    "mapbox-gl": "^3.15.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-big-calendar": "^1.19.4",
    "react-calendar": "^6.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.63.0",
    "react-i18next": "^15.7.4",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-organizational-chart": "^2.2.1",
    "react-router-dom": "^7.9.2",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "react-window": "^2.2.0",
    "recharts": "^3.2.1",
    "remark-gfm": "^4.0.1",
    "supercluster": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.5",
    "@eslint/js": "^9.36.0",
    "@percy/cypress": "^3.1.6",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react": "^9.1.9",
    "@storybook/react-vite": "^9.1.9",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "babel-jest": "^29.7.0",
    "cross-fetch": "^4.1.0",
    "cypress": "^15.3.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.4.0",
    "i18next-cli": "^1.11.1",
    "i18next-scanner": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.8.2",
    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.13",
    "tinycolor2": "^1.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "wait-on": "^8.0.5",
    "web-streams-polyfill": "^4.2.0",
    "ws": "^8.18.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ],
    "**/*.{css,scss,md,json}": [
      "npx prettier --write"
    ]
  }
}
